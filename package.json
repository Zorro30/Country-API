    "co": "^4.6.0",
    "everyconfig": "^1.0.2",
    "fs-promise": "^1.0.0",
    "lodash": "^4.17.21",
    "restify": "^8.1.0",
    "restify-errors": "^7.0.0",
    "restify-plugins": "^1.6.0",
    "restify-swagger-jsdoc": "^3.2.4",
    "swagger-jsdoc": "^6.0.2",
