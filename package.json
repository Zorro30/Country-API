  "scripts": {
    "start": "nodemon src/server.js",
    "test": "NODE_ENV=test mocha --reporter spec --ui bdd 'test/unit/**/*-test.js' --exit",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=cobertura npm test",
